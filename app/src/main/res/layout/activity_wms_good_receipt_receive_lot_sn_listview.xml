<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        style="@style/WmsTextStartMargin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvGrLotGridSkuLevel2"
                    style="@style/WmsTextViewGridTitle"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/SKU_LEVEL" />

                <TextView
                    android:id="@+id/tvGrLotGridSkuLevel"
                    style="@style/WmsTextViewGridTitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Sku Level" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvGrLotGridSkuNum2"
                    style="@style/WmsTextViewGridTitle"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/SKU_NUM" />

                <TextView
                    android:id="@+id/tvGrLotGridSkuNum"
                    style="@style/WmsTextViewGridTitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Sku Num" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvGrLotGridQty2"
                    style="@style/WmsTextViewGridContext"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/QTY" />

                <TextView
                    android:id="@+id/tvGrLotGridQty"
                    style="@style/WmsTextViewGridContext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/QTY" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvGrLotGridUom2"
                    style="@style/WmsTextViewGridContext"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/UOM_TITLE" />

                <TextView
                    android:id="@+id/tvGrLotGridUom"
                    style="@style/WmsTextViewGridContext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="UOM" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    style="@style/WmsTextViewGridContext"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/MFG_DATE" />

                <TextView
                    android:id="@+id/tvGrDetGridMfgDate"
                    style="@style/WmsTextViewGridContext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="MFG_DATE" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    style="@style/WmsTextViewGridContext"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/EXP_DATE" />

                <TextView
                    android:id="@+id/tvGrDetGridExpDate"
                    style="@style/WmsTextViewGridContext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="EXP_DATE" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_common_chevron_right_black_32dp" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


    </LinearLayout>

</LinearLayout>