        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView622"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/CALL_FIX_TYPE" />

            <TextView
                android:id="@+id/textView632"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="：" />

            <TextView
                android:id="@+id/tvCallFixType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView621"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/FAIL_START_DATE" />

            <TextView
                android:id="@+id/textView631"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="：" />

            <TextView
                android:id="@+id/tvFailStartDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView620"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/FAIL" />

            <TextView
                android:id="@+id/textView630"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="：" />

            <TextView
                android:id="@+id/tvFailId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="TextView" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0.2"
        android:layout_gravity="center_horizontal"
        android:orientation="vertical">

        <Switch
            android:id="@+id/swUpload"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:checked="true"
            android:text="@string/IS_UPLOAD" />
    </LinearLayout>
</LinearLayout>